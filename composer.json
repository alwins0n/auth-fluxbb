{
    "name": "alwins0n/auth-fluxbb",
    "description": "A package for external authentication with FluxBB 1.5.",
    "keywords": [
        "fluxbb",
        "authentication",
        "laravel"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Franz Liedke",
            "email": "franz@develophp.org"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "illuminate/support": "~5",
        "illuminate/auth": "~5",
        "symfony/security": "~2.6"
    },
    "require-dev": {
        "mockery/mockery": "~0.9",
        "phpunit/phpunit": "~4.0"
    },
    "autoload": {
        "psr-0": {
            "FranzLiedke\\AuthFluxBB": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "config": {
        "preferred-install": "dist"
    },
    "minimum-stability": "dev"
}
